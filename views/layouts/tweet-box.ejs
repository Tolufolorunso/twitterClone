<article class="timeline">
  <nav class="timeline__nav px-2 py-1">
    <div class="nav__profile-img profile">
      <img src="/static/images/logo-blue.png" alt="">
    </div>
    <h1>Home</h1>
    <span class="material-symbols-outlined">
      hub
    </span>
  </nav>
  <div class="timeline__tweetPost px-2">
    <div class="nav__profile-img">
      <img src="/static/images/logo-blue.png" alt="">
    </div>
    <div class="timeline__tweetPost--post">
      <textarea name="tweet" id="tweet-field" cols="30" rows="2" placeholder="What's happening?"></textarea>
      <button class="timeline__tweetPost--everyone">
        <span class="material-symbols-outlined">
          public
        </span>
        Everyone can reply
      </button>
      <div class="border"></div>
      <div class="timeline__tweetPost--media">
        <label for="upload">
          <span class="material-symbols-outlined">
            perm_media
          </span>
          <input type="file" name="file" id="upload">
        </label>
        <span class="material-symbols-outlined">
          gif_box
        </span>
        <span class="material-symbols-outlined">
          ballot
        </span>
        <span class="material-symbols-outlined">
          mood
        </span>
        <span class="material-symbols-outlined">
          auto_schedule
        </span>
        <button class="btn post-tweet" disabled>Tweet</button>
      </div>
    </div>
  </div>
